<div class="page-header">
  <h1>
    Plancontents
    <a href="/plancontents/new" class="btn btn-lg btn-success">New Plancontent</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Mealtime</th>
        <th>Meal</th>
        <th>Actions</th>
      </tr>

      <% @plancontents.each do |plancontent| %>
      <tr>
        <td><%= plancontent.mealtime_id %></td>
        <td><%= plancontent.meal_id %></td>
        <td>
          <a href="/plancontents/<%= plancontent.id %>" class="btn btn-primary">Show</a>
          <a href="/plancontents/<%= plancontent.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_plancontent/<%= plancontent.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
